# contact stress between cylinder and brick, based on a non-parametric model by
# dichtstoff
valu radius 50
valu height 60
valu width 120

pnt p1 radius 0 0
pnt p2 0 0 0
pnt p3 radius radius 0
# undocumented line bias
line peri p2 p3 p1 80 1.05
line l1 p2 p1 80 1.05
line l2 p3 p1 40 1
surf s1 l2 peri l1
move all tra height 0 0

seto block
pnt p4 height 0 0
valu neg -1
valu neg_height * neg height
swep block block tra neg_height 0 0 80
bia block 8
swep block block tra 0 width 0 120
seta lb l L003 L004
bia lb 8

elty all qu8e
mesh all
send all abq

rot -z
view elem
plot f all n
frame
seta ! all
# plot f all n
hcpy png
sys mv hcpy_1.png parts.png
zoom 10
tra u 55
tra l 5
# this does not work, should write the zoomed plot
hcpy png
sys mv hcpy_2.png parts-zoom.png


SETA scyl l peri
comp scyl do
comp scyl do
send scyl abq sur

seta sblk l L003
comp sblk do
comp sblk do
send sblk abq sur

seta y0 l L001 l1
comp y0 do
comp y0 do
send y0 abq spc 2

seta x0 l L004
comp x0 do
comp x0 do
send x0 abq spc 1


seta control l l2
comp control do
comp control do
# sending pressure load does not work
# send control abq pres 1
# workaround: displacement control
send control abq spc 1 -1
